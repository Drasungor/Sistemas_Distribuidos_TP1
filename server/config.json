{
    "general": {
        "rabbit_address": "rabbitmq",
        "rabbit_port": "15672",
        "general_subscription_routing_key": "-1"
    },
    "MOM": {
        "broker_address": "rabbitmq",
        "connections": {
            "accepter": {
                "sends_to": ["likes_filter", "trending_days_filter"]
            },
            "likes_filter": {
                "sends_to": ["funny_filter", "views_sum"]
            },
            "funny_filter": {
                "sends_to": ["duplication_filter"]
            },
            "duplication_filter": {
                "sends_to": ["accepter"]
            },
            "views_sum": {
                "sends_to": ["max_views_day"]
            },
            "max_views_day": {
                "sends_to": ["accepter"]
            },
            "trending_days_filter": {
                "sends_to": ["countries_amount_filter"]
            },
            "countries_amount_filter": {
                "sends_to": ["thumbnails_downloader"]
            },
            "thumbnails_downloader": {
                "sends_to": ["accepter"]
            }
        }
    },
    "accepter":{
        "bound_port": 8080,
        "listen_backlog": 10,
        "receives_from": ["duplication_filter", "max_views_day", "thumbnails_downloader"],
        "processes_amount": 3,
        "kept_columns": ["video_id", "title", "category", "trending_date", "tags", "views", "likes", "thumbnail_link", "country"],
        "indexes": {
            "video_id": 0,
            "title": 1,
            "category": 5,
            "trending_date": 6,
            "tags": 7,
            "views": 8,
            "likes": 9,
            "thumbnail_link": 12,
            "country": 16
        }
    },
    "likes_filter": {
        "computers_amount": 2,
        "likes_min": 5000000,
        "receives_from": "accepter",
        "hashed_by": ["video_id"],
        "indexes": {
            "video_id": 0,
            "title": 1,
            "category": 2,
            "trending_date": 3,
            "tags": 4,
            "views": 5,
            "likes": 6,
            "thumbnail_link": 7,
            "country": 8
        },
        "kept_columns": ["video_id", "title", "category", "trending_date", "tags", "views"]
    },
    "funny_filter": {
        "computers_amount": 2,
        "tag": "funny",
        "receives_from": "likes_filter",
        "hashed_by": ["video_id"],
        "indexes": {
            "video_id": 0,
            "title": 1,
            "category": 2,
            "trending_date": 3,
            "tags": 4,
            "views": 5
        },
        "kept_columns": ["video_id", "title", "category"]
    },
    "duplication_filter": {
        "computers_amount": 2,
        "receives_from": "funny_filter",
        "hashed_by": ["video_id"],
        "indexes": {
            "video_id": 0,
            "title": 1,
            "category": 2
        }
    },
    "views_sum": {
        "computers_amount": 2,
        "receives_from": "likes_filter",
        "hashed_by": ["trending_date"],
        "indexes": {
            "video_id": 0,
            "title": 1,
            "category": 2,
            "trending_date": 3,
            "tags": 4,
            "views": 5
        }
    },
    "max_views_day": {
        "receives_from": "views_sum",
        "computers_amount": 1
    },
    "trending_days_filter": {
        "computers_amount": 2,
        "receives_from": "accepter",
        "min_trending_days": 21,
        "hashed_by": ["video_id", "country"],
        "indexes": {
            "video_id": 0,
            "title": 1,
            "category": 2,
            "trending_date": 3,
            "tags": 4,
            "views": 5,
            "likes": 6,
            "thumbnail_link": 7,
            "country": 8
        },
        "kept_columns": ["video_id", "thumbnail_link", "country"]
    },
    "countries_amount_filter": {
        "computers_amount": 2,
        "receives_from": "trending_days_filter",
        "hashed_by": ["video_id"],
        "indexes": {
            "video_id": 0,
            "thumbnail_link": 1,
            "country": 2
        },
        "kept_columns": ["video_id", "thumbnail_link"]
    },
    "thumbnails_downloader": {
        "computers_amount": 2,
        "receives_from": "countries_amount_filter",
        "hashed_by": ["video_id"],
        "indexes": {
            "video_id": 0,
            "thumbnail_link": 1
        }
    }
}