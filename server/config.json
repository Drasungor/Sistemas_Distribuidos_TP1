{
    "general": {
        "rabbit_address": "rabbitmq",
        "rabbit_port": "15672",
        "indexes": {
            "video_id": 0,
            "title": 1,
            "trending_date": 2,
            "tags": 7,
            "views": 8,
            "likes": 9,
            "category_name": 10,
            "country": 11
        },
        "EOF_subscription_routing_key": "-1"
    },
    "MOM": {
        "broker_address": "rabbitmq",
        "connections": {
            "accepter": {
                "sends_to": ["likes_filter", "trending_days_filter"]
            },
            "likes_filter": {
                "sends_to": ["funny_filter", "views_sum"]
            },
            "funny_filter": {
                "sends_to": ["duplication_filter"]
            },
            "duplication_filter": {
                "sends_to": ["accepter"]
            },
            "views_sum": {
                "sends_to": ["max_views_day"]
            },
            "max_views_day": {
                "sends_to": ["accepter"]
            },
            "trending_days_filter": {
                "sends_to": ["countries_amount_filter"]
            },
            "countries_amount_filter": {
                "sends_to": ["thumbnails_downloader"]
            },
            "thumbnails_downloader": {
                "sends_to": ["accepter"]
            }
        }
    },
    "accepter":{
        "bound_port": 8080,
        "listen_backlog": 10,
        "receives_from": ["duplication_filter", "max_views_day", "thumbnails_downloader"],
        "processes_amount": 3
    },
    "likes_filter": {
        "computers_amount": 2,
        "likes_min": 5000000,
        "receives_from": "accepter",
        "hashed_by": ["video_id"]
    },
    "funny_filter": {
        "computers_amount": 2,
        "tag": "funny",
        "receives_from": "likes_filter",
        "hashed_by": ["video_id"]
    },
    "duplication_filter": {
        "computers_amount": 2,
        "receives_from": "funny_filter",
        "hashed_by": ["video_id"]
    },
    "views_sum": {
        "computers_amount": 2,
        "receives_from": "likes_filter",
        "hashed_by": ["trending_date"]
    },
    "max_views_day": {
        "receives_from": "views_sum",
        "computers_amount": 2
    },
    "trending_days_filter": {
        "computers_amount": 2,
        "receives_from": "accepter",
        "hashed_by": ["video_id", "country"]
    },
    "countries_amount_filter": {
        "computers_amount": 2,
        "receives_from": "trending_days_filter",
        "hashed_by": ["video_id"]
    },
    "thumbnails_downloader": {
        "computers_amount": 2,
        "receives_from": "countries_amount_filter",
        "hashed_by": ["video_id"]
    }
}